{
  "name": "server-control-s3",
  "version": "0.0.14",
  "author": {
    "name": "Jim Lake"
  },
  "license": "MIT",
  "description": "Easy updating of Amazon AWS instances from s3 packages",
  "keywords": [
    "aws",
    "update",
    "asg",
    "ec2"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jim-lake/server-control-s3.git"
  },
  "files": [
    "dist/*",
    "scripts/*"
  ],
  "main": "dist/server_control.js",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "lint": "eslint src --ext ts && eslint test --ext ts && eslint example --ext js",
    "pretty": "prettier --write \"src/*.ts\" \"example/*.js\" \"test/*.ts\"",
    "test": "npm run build && npm run test-only",
    "test-only": "NODE_OPTIONS='--import=tsx' mocha --require test/local_setup.ts \"test/*.test.ts\""
  },
  "dependencies": {
    "@aws-sdk/client-auto-scaling": "^3.525.0",
    "@aws-sdk/client-ec2": "^3.525.0",
    "@aws-sdk/client-s3": "^3.525.0",
    "async": "3.2.6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/async": "^3.2.24",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "chai": "^5.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "mocha": "^11.7.1",
    "rollup": "^4.44.2",
    "rollup-plugin-dts": "^6.2.1",
    "supertest": "^7.1.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
